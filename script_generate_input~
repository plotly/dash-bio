#!/bin/bash

echo "Script Usage: script_generate_input <pdb_file_name>"

let pdb_file = $1

#Create input data file
python pdbParse.py pdb_file > data/input_data1.js

#Create input style file
python pdbStyle1.py pdb_file > data/input_style1.js

#create sphere style file
cat input_style1.js | sed 's/cartoon/sphere/g' > data/input_style_sphere.js
